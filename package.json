{
  "name": "Excalibur-Assistant-consumer",
  "version": "0.0.1",
  "main": "src/index.js",
  "engines": {
    "node": "10.13.0",
    "yarn": "1.12.1"
  },
  "license": "MIT",
  "dependencies": {
    "amqplib": "*",
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "fastify": "^3.19.1"
